{
  "name": "social-backend",
  "version": "0.0.1",
  "description": "Backend for a social media app",
  "main": "app.js",
  "scripts": {
    "start": "ts-node src/app.ts",
    "dev": "nodemon --exec ts-node src/app.ts",
    "build": "webpack --config webpack.config.js",
    "start:prod": "node dist/app.js",
    "lint": "eslint . --ext .ts",
    "migrations:run": "npx ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/orm/dataSource.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SantiagoPintos/social-backend.git"
  },
  "keywords": [
    "backend",
    "social"
  ],
  "author": "Santiago Pintos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SantiagoPintos/social-backend/issues"
  },
  "homepage": "https://github.com/SantiagoPintos/social-backend#readme",
  "dependencies": {
    "@types/cors": "^2.8.17",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "reflect-metadata": "^0.2.1",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-filter-warnings-plugin": "^1.2.1"
  }
}
